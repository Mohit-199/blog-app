<%- include("Partials/header"); -%>

    <h1>Compose</h1>
    <form action="/updateCompose" method="POST">
        <div class="form-group">
            <h4>Title</h4>
            <input class="form-control" type="text" name="updatedTitle" value="<%= Titles%>" autocomplete="off">
            <h4>Post</h4>
            <textarea class="form-control" type="textarea" name="updatedPost" rows="10"><%= Body%></textarea>
        </div>
        <button class="btn btn-primary btn-lg" type="submit" name="buttonId" value=<%=ObjectId %> >Publish</button>

    </form>

    <%- include("Partials/footer"); -%>